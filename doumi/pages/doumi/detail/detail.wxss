.detail-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #e5e5e5;
}
 .detail-container .detail-header {
  width: 100%; 
}
 .detail-container .detail-header image {
   width: 100%;
   height: 50px; 
}
.content{
  background: #fff;
  margin: 10px 0;
}
.navigator{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  height:100%;
  padding: 15px 10px;
  box-sizing: border-box;
}
.seccenter .secleft{
  position: relative;
  width: 60px;
  height: 60px;
  padding-bottom: 4px;
  overflow: hidden;
}
.secleft image{
  width: 56px;
  height: 56px;
  margin: 0 auto;
  border-radius: 10px;
  border: 1px solid #e5e5e5;
}
.secright{
  display: flex;
  flex-direction: column;
  flex: 1;

  box-sizing: border-box;
  padding-left:4px;
}
.secright-t{
  width: 100%;
  overflow: hidden;
}
.secright-t span{
  width: 80%;
  float: left;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 24px;
  font-size: 16px;
  color: #282828;
  font-weight: 600;
}
.secright-t b{
  float: right;
  height: 16px;
  padding: 0 2px;
  font-size: 12px;
  line-height: 16px;
  margin-right: 8px;
  border-radius: 2px;
  color: #fff;
  background: #f2f2f2 url("../../../image/sec03.png") no-repeat;
  background-size: cover;
}
.secright-c image{
  width: 11px;
  height: 12px;
  margin-right: 4px;
}
.secright-c b{
  float: left;
  margin-top:3px;
  line-height: 1;
  font-size: 12px;
  color: #808080;
}
.secright-c i{
  float: right;
  font-size: 20px;
  color: #ea4335;
  font-family: Arial;
}
.secright-b{
  width: 100%;
  overflow: hidden;
}
.secright-b i{
  float: left;
  width: 62px;
  height: 12px;
  color: #34a853;
  text-align: center;
  line-height: 12px;
  border: 1px solid #34a853;
  margin-left: 6px;
  font-size: 10px;
}
.secright-b em{
  float: right;
  width: 83px;
  height: 13px;
  color: #888;
  font-size: 14px;
  text-align: center;
}
.gift{
  width:100%;
  height: 36px;
  line-height: 36px;
  color: #f60;
  padding: 0 12px;
  font-size: 14px;
  box-sizing: border-box;
  background: #fff;
  font-weight: 600;
  margin-bottom: 10px;
}
.gift span{
  float: right;
}
.section{
  background: #fff;
  padding: 10px; 
  margin-bottom: 10px;
}
.section span{
  display: block;
  margin-bottom: 5px;
  color: #282828;
  font-size: 18px;
  line-height: 1.5;
  font-weight: 600;
}
.section b{
  font-size: 14px;
  color: #888;
  margin-right: 8px;
  line-height: 30px;
}
.section em{
  font-size: 14px;
  line-height: 30px;
}
.section i{
  color: #34a853;
  font-size: 12px;
}
.safe strong{
  display: block;
  width: 62px;
  height: 12px;
  color: #34a853;
  text-align: center;
  line-height: 12px;
  border: 1px solid #34a853;
  font-size: 10px;
}
.place{
   background: #fff;
   padding: 10px;
   line-height: 24px;
}
.ptop{
  width: 100%;
  border-bottom: 1px solid #ddd;
  overflow: hidden;
}
.ptop b{
  float: left;
  font-size: 18px;
  font-weight: 600;
}
.ptop span{
  float: right;
  color: #f60;
  font-size: 12px;
}
.pbottom{
  padding-top: 5px;
}
.pbottom b{
  font-size: 12px;
  color: #888;
}
.pbottom i{
  font-size: 14px;
}
.contop{
  font-size: 16px;
  color: #34a853;
  padding-left: 10px;
  line-height: 30px;
  font-weight: 600;
  border-bottom: 1px solid #ddd;
}
